<?xml version='1.0' encoding='utf-8'?>
<widget id="cnom.stoneagetechnologies.happyquest" version="1.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <feature name="PushAppsPlugin">
        <param name="android-package" value="com.pushapps.phonegap.PushAppsPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Happy Quest</name>
    <description>
        A task management framework used to maximum flow resulting in increased productivity 
    </description>
    <author email="mobilestoneagetechnologies@gmail.com" href="http://www.stoneagetechnologies.com/happyquest">
        StoneAge Technologies
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
