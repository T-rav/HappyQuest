<?xml version='1.0' encoding='utf-8'?>
<widget id="com.stoneagetechnologies.happytask" version="0.9.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="SQLitePlugin">
        <param name="android-package" value="org.pgsqlite.SQLitePlugin" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <name>HappyQuest</name>
    <description>
        A micro task framework maximizing flow time.
    </description>
    <author email="dev@cordova.apache.org" href="http://stoneagetechnologies.com/happyquest">
        StoneAge Technologies
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
